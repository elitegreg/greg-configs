Host *
  Cipher aes128-ctr
  ServerAliveInterval 30
  ControlMaster auto
  ControlPersist 1m
  ControlPath ~/.ssh/controlsessions/%r@%h:%p

Host vmserver 
  HostName vmserver.tuxsoft.net
  Port 2222
  ForwardAgent yes
  AddressFamily inet

Host ham
  ForwardX11 yes
  AddressFamily inet

Host gateway
  User root

Host vmserver-ipmi
  #HostName vmserver.tuxsoft.net
  #Port 2224
  User ADMIN
  KexAlgorithms=+diffie-hellman-group1-sha1
  AddressFamily inet

Host bwing
  HostName bwing.tuxsoft.net
  Port 2225
  DynamicForward localhost:8080
  AddressFamily inet

Host dev-hq1-sv001
  User gmitchell

Host gerrit.corp.maystreet.com
  User gmitchell
  KexAlgorithms=+diffie-hellman-group1-sha1

Host sftp05.maystreet.com
  User gmitchell
